	citeulike-linkout-3 = {http://www.hubmed.org/display.cgi?uids=10387070},
	doi = {10.1021/bi990510+},
	issn = {0006-2960},
	journal = {Biochemistry},
	keywords = {amyloid, beta-sheet, exp, peptides},

	number = {26},
	pages = {8246--8252},
	posted-at = {2009-08-18 22:40:17},
	priority = {2},
	title = {Water-soluble beta-sheet models which self-assemble into fibrillar structures.},

	volume = {38},
	year = {1999}
}

@article{ff_bithph_raos_2003,
	abstract = {We present the results of ab initio calculations of the torsion potential of 2,2 ? -bithiophene, using the MP2, CCSD, CCSD(T) and B3LYP electron correlation methods. Special emphasis is given to the systematic investigation of basis set effects by the use of correlation-consistent bases. We find that the MP2 and coupled-cluster potential energy curves agree well with each other, for a given basis set. However, unlike B3LYP, they are rather sensitive to the quality of the basis. The previously observed disagreement between the B3LYP and MP2 methods, especially on the degree of ring coplanarity in the minimum-energy  cis  and  trans  conformations, is considerably reduced upon adoption of a large and flexible basis set for the latter.},
